{
  "extends": "@tsconfig/react-native/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@assets/*": [
        "src/assets/*"
      ],
      "@config/*": [
        "src/config/*"
      ],
      "@module/*": [
        "src/module/*"
      ],
      "@store/*": [
        "src/store/*"
      ],
      "@navigation/*": [
        "src/navigation/*"
      ],
    }
  }
}
